{
  "private": true,
  "name": "fia-discord-bot",
  "version": "3.0.0",
  "description": "Bot that send updates to discord when new FIA documents arrive.",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "serve": "node build/main.js",
    "start": "nodemon --exec node --loader ts-node/esm/transpile-only src/main.ts",
    "prepare": "husky install",
    "lint": "eslint --max-warnings=0 ./src"
  },
  "keywords": [
    "FIA",
    "bot"
  ],
  "author": "Markus Ort",
  "license": "ISC",
  "dependencies": {
    "@discordjs/rest": "^0.4.1",
    "bufferutil": "^4.0.6",
    "discord-api-types": "^0.32.0",
    "discord.js": "^13.6.0",
    "erlpack": "github:discord/erlpack",
    "mongodb": "^4.2.0",
    "node-fetch": "^3.0.0",
    "require-all": "^3.0.0",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/node": "^18.6.2",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">= 8.0.0"
  }
}
